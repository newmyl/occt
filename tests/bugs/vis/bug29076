puts "============"
puts "0029076: Visualization - implement element shrinking Shader"
puts "============"
puts ""

pload MODELING VISUALIZATION
vinit View1
psphere s0 2
psphere s1 1
psphere s2 1
psphere s3 1
psphere s4 1
psphere s5 1
vdisplay -dispMode 1 s0 s1 s2 s3 s4 s5
vsetlocation s1 -1.3 -1.3 1.3
vsetlocation s2 1.3 1.3 1.3
vsetlocation s3 1.3 -1.3 0
vsetlocation s4 1.3 1.3 -1.3
vsetlocation s5 -1.3 -1.3 -1.3
vrotate 0.0 0.5 0.0
vrotate 0.4 0.0 0.0
vfit

vaspects s0 -setShadingModel color
vaspects s1 -setShadingModel flat
vaspects s2 -setShadingModel phong
vaspects s3 -setShadingModel gouraud
vaspects s4 -setShadingModel flat

vsetinteriorstyle s0 wireframe
vsetinteriorstyle s1 6
vsetedgetype s1 -width 2
vsetinteriorstyle s2 7
vsetedgetype s2 -scalefactor 0.5
vsetinteriorstyle s3 8
vsetedgetype s3 -color 1.0 1.0 1.0
vsetinteriorstyle s4 3
vsetinteriorstyle s5 4

vdump $imagedir/${casename}.png
